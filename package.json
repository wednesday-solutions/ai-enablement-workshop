{
  "name": "stagepass",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r run dev",
    "dev:server": "pnpm --filter @stagepass/server run dev",
    "dev:web": "pnpm --filter @stagepass/web run dev",
    "build": "pnpm -r run build",
    "seed": "pnpm --filter @stagepass/server run seed"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}